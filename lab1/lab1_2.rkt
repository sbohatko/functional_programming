(define (func a b) ; процедура виведення чисел у порядку зростання/зменшення
   (display a); виведення числа
   (display "")
   (cond ((> a b) ; якщо а більше b
         (func (- a 1) b)); виклик рекурсії у порядку спадання
         ((< a b) ; якщо a менше b
          (func (+ a 1) b)))); виклик рекурсії у порядку зростання
(func 7 3) ; виклик процедури